{
  "name": "dcbd-react-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run start:dev\" \"npm run client\""
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html, tsx",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@server": "dist/server",
    "@common": "dist/common"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/morgan": "^1.9.1",
    "command-line-args": "^5.1.1",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status-codes": "^2.1.4",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
